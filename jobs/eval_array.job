#!/bin/bash

#SBATCH --partition=gpu_titanrtx_shared_course
#SBATCH --gres=gpu:1
#SBATCH --job-name=EvalNLI
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=3
#SBATCH --time=04:00:00
#SBATCH --mem=32000M
#SBATCH --array=2-4%1
#SBATCH --output=jobs/slurm_output/eval_%A_%a.out

module purge
module load 2021
module load Anaconda3/2021.05

ARRAY_FILE=$HOME/NLI/jobs/eval_array.txt

cd $HOME/NLI/
source activate nli

srun python -u nli/eval.py $(head -$SLURM_ARRAY_TASK_ID $ARRAY_FILE | tail -1)
srun python -u nli/results.py $(head -$SLURM_ARRAY_TASK_ID $ARRAY_FILE | tail -1)
